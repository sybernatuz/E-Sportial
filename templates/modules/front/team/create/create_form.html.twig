{% import "modules/macro/flash_message.html.twig" as flashes %}
{{ flashes.get_messages() }}
{{ form_start(form) }}
<div class="row">
    <div class="input-field col s12 m8 l8 xl8">
        {{ form_widget(form.name) }}
        {{ form_label(form.name) }}
        <div class="form-errors">
            {{ form_errors(form.name) }}
        </div>
    </div>
    <div class="input-field col s4 m4 l4 xl4">
        {{ form_widget(form.country) }}
        {{ form_label(form.country) }}
    </div>
    <div class="input-field col s12 m12 l12 xl12">
        {{ form_widget(form.description) }}
        {{ form_label(form.description) }}
        <div class="form-errors">
            {{ form_errors(form.description) }}
        </div>
    </div>
    <div class="file-field input-field col s12 m12 l12 xl12">
        <div class="btn">
            <span>Logo</span>
            {{ form_widget(form.logoFile) }}
        </div>
        <div class="file-path-wrapper">
            {{ form_widget(form.logoPath) }}
        </div>
        <div class="form-errors">
            {{ form_errors(form.logoFile) }}
        </div>
    </div>
</div>
<div class="row">
    <div class="col s12 m12 l12 xl12 right-align">
        {{ form_widget(form.submit) }}
    </div>
</div>
{{ form_end(form) }}