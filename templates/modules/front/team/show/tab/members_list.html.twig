<div id="member-list" class="container-fluid" style="margin-top:30px;">
    <div class="row">
        <div class="col s3 m3 l3 xl3" style="height: 300pt; overflow: auto">
            {% for user in team.users %}
                {% include "modules/front/team/show/tab/member.html.twig" with {'user': user} %}
            {% endfor %}
        </div>
        <div class="col s9 m9 l9 xl9">
            <div class="row">
                {% include "modules/front/team/show/tab/member_details.html.twig" with {'user': team.users|first, 'team': team} %}
            </div>
        </div>
    </div>
</div>