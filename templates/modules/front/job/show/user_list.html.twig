{% for user in job.applicants %}
    <div class="row">
        <div class="col s6 m6">
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                    <span class="card-title">
                        <a target="_blank" rel="noopener noreferrer" href="{{ path('app_user_show', {'slug' : user.slug }) }}">
                            {{ user.username }}
                        </a>
                        <i class="material-icons">email</i>
                        <a href="{{ path('app_job_delete_user', {'jobId' : job.id, 'userId' : user.id}) }}">
                            <i class="material-icons">delete</i>
                        </a>
                    </span>
                </div>
            </div>
        </div>
    </div>
{% endfor %}